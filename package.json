    "test": "mocha",
    "types": "tsc"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.14.11",
    "c8": "^10.1.2",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.8.0",
    "mocha": "^10.7.0",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.5.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.19.0",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
